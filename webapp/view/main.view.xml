<mvc:View
		height="100%"
		controllerName="gfex.petrobras.fornmanager.controller.main"
		xmlns:core="sap.ui.core"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.m">
		<NavContainer id="wizardNavContainer">
			<pages>
				<Page
				id="wizardContentPage"
				showHeader="true">
					<content>
						
						<mvc:XMLView
							id="wizardViewLinear"
							async="true"
							visible="{= ${/selectedShowCase} === 'linear' }"
							viewName="gfex.petrobras.fornmanager.view.Linear" />
							<mvc:XMLView
							id="wizardViewReview"
							async="true"
							visible="{= ${/selectedShowCase} === 'review' }"
							viewName="gfex.petrobras.fornmanager.view.Review" />
						</content>
					</Page>
				
			</pages>
		</NavContainer>
</mvc:View>

